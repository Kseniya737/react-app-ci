    "@babel/cli": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@cucumber/cucumber": "^8.5.1",
    "@faker-js/faker": "^7.4.0",
    "@playwright/test": "^1.24.2",
    "cucumber-html-reporter": "^5.5.0",
    "dotenv": "^16.0.1",
    "playwright": "^1.24.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
